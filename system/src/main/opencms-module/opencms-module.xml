            <class/>
            <import-script><![CDATA[# sets access control entries  and properties on folders not exclusively belonging to this module
chacc '/system/' 'ROLE' 'WORKPLACE_USER' '+I+R'
chacc '/system/modules/' 'ROLE' 'WORKPLACE_USER' '+I+R+V'
createFolder '/system/' 'config'
writeProperty '/system/config/' 'export' 'false'
writeProperty '/system/workplace/' 'content-encoding' 'UTF-8'
writeProperty '/system/workplace/' 'export' 'false'
writeProperty '/system/workplace/' 'login-form' '/system/login/index.html'
chacc '/system/workplace/' 'GROUP' 'ALL_OTHERS' '+I+O'
chacc '/system/workplace/' 'ROLE' 'WORKPLACE_USER' '+I+O+R+V'
createFolder '/system/config/' 'notification'
writeProperty '/system/config/notification/' 'Title' 'Content Notification'
writeProperty '/system/workplace/commons/' 'cache' 'false'
# create folders for categories and set title / german title
createFolder '/system/' 'categories'
writeProperty '/system/categories/' 'Title' 'Global categories'
writeProperty '/system/categories/' 'Title_de' 'Globale Kategorien'
# create folders for user images
chacc '/system/userimages/' 'ROLE' 'ELEMENT_AUTHOR' '+I+O+R'
writeProperty '/system/workplace/editors/' 'cache' 'false'
chacc '/system/workplace/editors/' 'GROUP' 'ALL_OTHERS' '+I+O'
chacc '/system/workplace/editors/' 'ROLE' 'ELEMENT_AUTHOR' '+I+O+R+V']]></import-script>
